{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TcpPort": 5000,
  "SharedHosting": {
    "Enabled": false,
    "MaxMemoryMB": 128,
    "MaxCpuTimeSeconds": 30,
    "MaxBackgroundTasks": 1,
    "AutoResetIntervalMinutes": 15,
    "KeepAliveIntervalSeconds": 30
  },
  "PoolSettings": {
    "ResetIntervalMinutes": 30,
    "CleanupIntervalMinutes": 5
  },
  "ServerSettings": {
    "MaxTasksPerAgent": 1,
    "TaskTimeoutMinutes": 60
  }
}
